---
"permalink": "/{{ slug }}/"
"layoutType": "page"
---
<!DOCTYPE html>
<html lang="en">
<head>
	<title>{{ title }}</title>
	{% include "headparts.njk" %}
</head>
<body class="page-template">

	<div class="site-wrapper">

		<header class="site-header">
			{% include "header.njk" %}
		</header>

		<main id="site-main" class="site-main outer">

			<div class="inner">

				<article class="post-full post page">

					<header class="post-full-header">
						<h1 class="post-full-title">{{ title }}</h1>
					</header>

					{% if main_image_filename %}
						{% include "main_image.njk" %}
					{% endif %}

					<section class="post-full-content">

						<div class="post-content">

<!-- BEGIN MARKDOWN OUTPUT -->
{{ content | safe }}
<!-- BEGIN MARKDOWN OUTPUT -->

						</div>

					</section>

				</article>

			</div>

		</main>

		{% include "footer.njk" %}

	</div>

	{% include "script.njk" %}

</body>
</html>
